{
  "hosting": {
    "predeploy": [
      "npm --prefix \"vue\" run lint",
      "npm --prefix \"vue\" run build"
    ],
    "public": "vue/dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
